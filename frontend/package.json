{
  "name": "test",
  "version": "0.18.4",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 4173",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test dev http-get://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci:firefox": "start-server-and-test dev http-get://127.0.0.1:4173/ 'cypress run --e2e'",
    "test:e2e:ci:chrome": "start-server-and-test dev http-get://127.0.0.1:4173/ 'cypress run --e2e --browser chrome'",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "ci": "yarn install --non-interactive --frozen-lockfile"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11",
    "Firefox > 66",
    "Chrome > 70"
  ],
  "dependencies": {
    "@influxdata/influxdb-client-browser": "^1.27.0",
    "@vue-leaflet/vue-leaflet": "^0.6.1",
    "axios": "^0.27.2",
    "bootstrap-icons": "^1.8.3",
    "idb": "^7.0.2",
    "leaflet": "^1.8.0",
    "meyda": "^5.5.1",
    "motion-sensors-polyfill": "^0.3.1",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-router": "^4.1.1"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.13",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.43",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "c8": "^7.12.0",
    "cypress": "^10.4.0",
    "eslint": "^8.19.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.2.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "sass": "^1.54.4",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.6",
    "typescript": "~4.7.4",
    "vite": "^2.9.14",
    "vite-plugin-pwa": "^0.12.3",
    "vitest": "^0.18.0",
    "vue-tsc": "^0.38.3"
  }
}
