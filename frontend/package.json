{
  "name": "websense",
  "version": "0.18.11",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 4173",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test dev http-get://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci:firefox": "start-server-and-test dev http-get://127.0.0.1:4173/ 'cypress run --e2e'",
    "test:e2e:ci:chrome": "start-server-and-test dev http-get://127.0.0.1:4173/ 'cypress run --e2e --browser chrome'",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false --skipLibCheck",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11",
    "Firefox > 66",
    "Chrome > 70"
  ],
  "dependencies": {
    "@influxdata/influxdb-client-browser": "^1.33.2",
    "@michigg/component-library": "^0.3.1",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.7",
    "bootstrap-icons": "^1.11.3",
    "idb": "^8.0.0",
    "leaflet": "^1.9.4",
    "meyda": "^5.6.2",
    "motion-sensors-polyfill": "^0.3.7",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.7.2",
    "@types/jsdom": "^21.1.6",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^1.2.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "c8": "^9.1.0",
    "cypress": "^13.6.3",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "sass": "^1.70.0",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-pwa": "^0.17.5",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27",
    "workbox-build": "^7.0.0",
    "workbox-window": "^7.0.0"
  }
}
