{
  "name": "test",
  "version": "0.18.5",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 4173",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test dev http-get://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci:firefox": "start-server-and-test dev http-get://127.0.0.1:4173/ 'cypress run --e2e'",
    "test:e2e:ci:chrome": "start-server-and-test dev http-get://127.0.0.1:4173/ 'cypress run --e2e --browser chrome'",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "ci": "yarn install --non-interactive --frozen-lockfile"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11",
    "Firefox > 66",
    "Chrome > 70"
  ],
  "dependencies": {
    "@influxdata/influxdb-client-browser": "^1.31.0",
    "@michigg/component-library": "^0.3.1",
    "@vue-leaflet/vue-leaflet": "^0.6.1",
    "@vueuse/core": "^9.9.0",
    "axios": "^1.1.3",
    "bootstrap-icons": "^1.9.1",
    "idb": "^7.1.0",
    "leaflet": "^1.7.1",
    "meyda": "^5.5.1",
    "motion-sensors-polyfill": "^0.3.1",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.0",
    "@types/leaflet": "^1.9.0",
    "@types/node": "^18.11.7",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.24.3",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.6.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "sass": "^1.55.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.1",
    "typescript": "~4.8.4",
    "vite": "^3.2.1",
    "vite-plugin-pwa": "^0.13.1",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.9"
  }
